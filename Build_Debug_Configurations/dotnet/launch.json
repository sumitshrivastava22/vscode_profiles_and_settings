	"version": "0.2.0",
	"configurations": [		
		{
			"name": ".NET Core Launch",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build solution",
			"program": "${workspaceFolder}/TeamsBotApi/bin/x64/Debug/net8.0/TeamsBotApi.dll",
			"args": [],
			"cwd": "${workspaceFolder}/TeamsBotApi/bin/x64/Debug/net8.0",
			"stopAtEntry": false,
			"console": "internalConsole",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"ASPNETCORE_URLS": "http://localhost:5114"
			},
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": "C#: TeamsBotApi [https]",
			"type": "dotnet",
			"request": "launch",
			"projectPath": "${workspaceFolder}\\TeamsBotApi\\TeamsBotApi.csproj",
			"launchConfigurationId": "TargetFramework=;https"
		},
		{
			"name": "C#: TeamsBotApi [http]",
			"type": "dotnet",
			"request": "launch",
			"projectPath": "${workspaceFolder}\\TeamsBotApi\\TeamsBotApi.csproj",
			"launchConfigurationId": "TargetFramework=;http"
		}
	]
}
