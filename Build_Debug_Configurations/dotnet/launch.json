    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (dll)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "custom: build Debug x64",
            "program": "${workspaceFolder}/TeamsBotApi/bin/x64/Debug/net8.0/TeamsBotApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/TeamsBotApi/bin/x64/Debug/net8.0",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5114"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },            
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}